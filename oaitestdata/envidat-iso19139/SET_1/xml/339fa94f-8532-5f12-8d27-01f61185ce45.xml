            <gmd:CI_Citation>
              <gmd:title>
                <gco:CharacterString>Tree	measurements 2002-2016 from the long-term irrigation experiment Pfynwald, Switzerland</gco:CharacterString>
              </gmd:title>
              <gmd:date>
                <gmd:CI_Date>
                  <gmd:date>
                    <gco:Date>2016-12-31</gco:Date>
                  </gmd:date>
                  <gmd:dateType>
