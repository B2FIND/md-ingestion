            <gmd:CI_Citation>
              <gmd:title>
                <gco:CharacterString>DISCHMEX - Impact of extreme land-surface heterogeneity on micrometeorology over spring snow-cover</gco:CharacterString>
              </gmd:title>
              <gmd:date>
                <gmd:CI_Date>
                  <gmd:date>
                    <gco:Date>2017-12-31</gco:Date>
                  </gmd:date>
